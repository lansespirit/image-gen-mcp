        "lighting": {
          "type": "string",
          "required": false,
          "default": "dramatic",
          "description": "Lighting style",
          "examples": ["golden hour sunlight", "ethereal moonlight", "dramatic rim lighting", "soft ambient glow"]
        },
        "color_palette": {
          "type": "string",
          "required": false,
          "default": "rich and vibrant",
